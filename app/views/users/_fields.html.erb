<%= render 'shared/error_messages', object: f.object %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :age %>
<%= f.select(:age, options_for_select((16..50))) %>

<%= f.label :gender %>
<%= f.select(:gender, options_for_select(([["Male", 1], ["Female", 2], ["Unspecified", 3]]))) %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, "Confirm Password" %>
<%= f.password_field :password_confirmation %>